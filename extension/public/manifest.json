{
  "short_name": "Your Extension Name",
  "name": "Your Extension Name",
  "icons": {
    "16": "logo_blue.ico",
    "48": "logo192.png",
   "128": "logo512.png"
  },
  "permissions": ["tabs","activeTab", "scripting", "declarativeContent"],
  "manifest_version": 3,
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Your Extension Name"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}